{
  "name": "clubassistant",
  "version": "2.6.1",
  "description": "Bot de DDLC para fanarts, citas, trivia, noticias y videos.",
  "main": "bot.js",
  "scripts": {
    "start": "node bot.js"
  },
  "dependencies": {
    "axios": "^1.7.3",
    "cheerio": "^1.0.0-rc.12",
